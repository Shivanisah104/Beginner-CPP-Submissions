#include <iostream>
using namespace std;
int main() 
{
 int NC,NA,A;
 cout<<"no.of classes held"<<endl;
 cin>>NC;
 cout<<"no of classes attend"<<endl;
 cin>>NA;
 A=(NA/NC)*100;
 if(A<75)
 {
 cout<<"student is not allowed to seat in the exam"<<endl;
 }
 else
 {
   cout<<"student is allowed to seat"<<A<<endl;
 }
 return 0;
}
